9. delete a specific node from a given linked list.


class Solution
{
    public:
    //Function to delete a node without any reference to head pointer.
    void deleteNode(Node *del)
    {
       Node* n=del->next;
       int temp=del->data;
       del->data=n->data;
       n->data=temp;
       del->next=n->next;
       free(n);
    }

};
